prefix=/usr/local
exec_prefix=${prefix}
libdir=/usr/local/lib
includedir=/usr/local/include

Name: absl_flags_config
Description: Abseil flags_config library
URL: https://abseil.io/
Version: head
Requires: absl_config = head, absl_flags_path_util = head, absl_flags_program_name = head, absl_core_headers = head, absl_strings = head, absl_synchronization = head
Libs: -L${libdir}  -labsl_flags_config
Cflags: -I${includedir} -Wno-float-conversion -Wno-implicit-float-conversion -Wno-implicit-int-float-conversion -Wno-implicit-int-conversion -Wno-shorten-64-to-32 -Wno-sign-conversion -Wno-unknown-warning-option -DNOMINMAX
